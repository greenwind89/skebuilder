<skeleton>
	<data>
		<skeleton_id>phpfox_default</skeleton_id>
		<replacement_class>ReplacementPhpfox</replacement_class>
		<description>a skeleton for Phpfox module with category</description>
	</data>

	<structure>
		<node type='folder' name='upload'>
			<node type='folder' name='include'>
				<node type='folder' name='xml'>
					<node type='file' name='[skebuilder:package_id].xml' template='phpfox_product_xml'/>
				</node>
			</node>

			<node type='folder' name='module'>
				<node type='folder' name='[skebuilder:module_name]'>
					<node type='folder' name='include'>
						<node type='folder' name='component'>

							<node type='folder' name='ajax'>
								<node type='file' name='index.html'/>
							</node>

							<node type='folder' name='block'>
								<node type='file' name='category.class.php' template='phpfox_block_category_class'/>
								<node type='file' name='index.html'/>
							</node>

							<node type='folder' name='controller'>
								<node type='folder' name='admincp'>
									<node type='folder' name='category'>
										<node type='file' name='index.class.php' template='phpfox_admincp_category_class_index'/>
										<node type='file' name='add.class.php' template='phpfox_admincp_category_class_add'/>
										<node type='file' name='index.html'/>
									</node>
									<node type='file' name='index.html'/>
								</node>
								<node type='file' name='index.html'/>
								<node type='file' name='index.class.php' template='phpfox_controller_default_class'/>
							</node>

							<node type='file' name='index.html'/>

						</node>
						<node type='folder' name='plugin'>
							<node type='file' name='index.html'/>
						</node>
						<node type='folder' name='service'>
							<node type='folder' name='category'>
								<node type='file' name='category.class.php' template='phpfox_service_category_category'/>
								<node type='file' name='process.class.php' template='phpfox_service_category_process'/>
								<node type='file' name='index.html'/>
							</node>
							<node type='file' name='index.html'/>
						</node>

						<node type='file' name='index.html'/>
					</node>

					<node type='folder' name='static'>
						<node type='folder' name='image'>
							<node type='file' name='index.html'/>
						</node>

						<node type='folder' name='css'>
							<node type='folder' name='default'>
								<node type='folder' name='default'>
									<node type='file' name='index.html'/>
								</node>
								<node type='file' name='index.html'/>
							</node>
							<node type='file' name='index.html'/>
						</node>

						<node type='folder' name='jscript'>
							<node type='file' name='admin.js' template='phpfox_javascript_admin_category'/>

							<node type='file' name='index.html'/>
						</node>
					</node>
					<node type='folder' name='template'>
						<node type='folder' name='default'>
							<node type='folder' name='block'>
								<node type='file' name='category.html.php' template='phpfox_block_category_html'/>
								<node type='file' name='index.html'/>
							</node>

							<node type='folder' name='controller'>
								<node type='folder' name='admincp'>
									<node type='folder' name='category'>
										<node type='file' name='index.html.php' template='phpfox_admincp_category_html_index'/>
										<node type='file' name='add.html.php' template='phpfox_admincp_category_html_add'/>
										<node type='file' name='index.html'/>
									</node>
									<node type='file' name='index.html'/>
								</node>
								<node type='file' name='index.html.php' template='phpfox_block_default_html'/>
								<node type='file' name='index.html'/>
							</node>
							<node type='file' name='index.html'/>
						</node>
					</node>
					<node type='folder' name='yninstall'>
						<node type='folder' name='versions'>
							<node type='file' name='v3.01.php' template='phpfox_yninstall_with_category'/>
							<node type='file' name='index.html'/>
						</node>
						<node type='file' name='index.html'/>
					</node>
					<node type='file' name='phpfox.xml' template='phpfox_phrase_setting_custom_category_xml'/>
				</node>
			</node>
		</node>

	</structure>
</skeleton>